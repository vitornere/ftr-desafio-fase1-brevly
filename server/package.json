{
  "name": "brevly-server",
  "version": "1.0.0",
  "description": "Brev.ly Link Shortener Server",
  "main": "dist/infra/http/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/infra/http/server.js",
    "dev": "tsx watch --env-file .env src/infra/http/server.ts",
    "lint": "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true --colors=off",
    "test": "dotenv -e .env.test -- vitest run",
    "test:watch": "dotenv -e .env.test -- vitest",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:migrate:test": "dotenv -e .env.test -- drizzle-kit migrate",
    "db:migrate:test:watch": "dotenv -e .env.test -- drizzle-kit migrate",
    "pretest": "pnpm run db:migrate:test",
    "postinstall": "lefthook install",
    "build": "tsup-node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.840.0",
    "@aws-sdk/lib-storage": "^3.840.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "csv-stringify": "^6.5.2",
    "drizzle-orm": "^0.44.2",
    "fastify": "^5.4.0",
    "fastify-type-provider-zod": "^5.0.1",
    "postgres": "^3.4.7",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@faker-js/faker": "^9.8.0",
    "@types/node": "^24.0.8",
    "@types/pg": "^8.15.4",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.4",
    "lefthook": "^1.11.14",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}