{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 22",
  "_version": "22.11.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "ESNext",
    "target": "ESNext",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}